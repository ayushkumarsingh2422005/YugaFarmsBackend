{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "Coupon"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Expiry": {
      "type": "datetime",
      "required": true
    },
    "Count": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": -1
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "coupon"
    },
    "Percentage": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "Value": {
      "type": "integer",
      "required": true,
      "default": 0
    }
  }
}
